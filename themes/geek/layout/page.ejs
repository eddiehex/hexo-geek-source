<div class="page-container">
  <% if (page.toc) { %>
    <div class="toc-card">
      <div class="toc-content">
        <%- toc(page.content, {list_number: false}) %>
      </div>
    </div>
  <% } %>
  
  <article class="page-content">
    <h1 class="page-title"><%= page.title %></h1>
    <div class="page-body">
      <%- page.content %>
    </div>
  </article>
</div>